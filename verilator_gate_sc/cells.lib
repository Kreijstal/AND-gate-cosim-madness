library(cells) {
  cell(INV) {
    pin(A) { direction: input; }
    pin(Y) { direction: output; function: "!A"; }
  }
  cell(AND2) {
    pin(A) { direction: input; }
    pin(B) { direction: input; }
    pin(Y) { direction: output; function: "A & B"; }
  }
  cell(DFF) {
    pin(D) { direction: input; }
    pin(CLK) { direction: input; }
    pin(Q) { direction: output; function: "D"; }
  }
}
